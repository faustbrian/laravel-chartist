{
    "name": "faustbrian/laravel-chartist",
    "description": "Chartist Integration with a Fluent Builder for Laravel 5",
    "keywords": ["laravel", "framework", "Laravel-Chartist", "Laravel Chartist", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de"
    }],
    "require": {
        "php": "^7.0",
        "faustbrian/laravel-service-provider": "^1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Chartist\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Chartist\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\Chartist\\ChartistServiceProvider"
            ],
            "aliases": {
                "Chartist": "BrianFaust\\Chartist\\Facades\\Chartist"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
